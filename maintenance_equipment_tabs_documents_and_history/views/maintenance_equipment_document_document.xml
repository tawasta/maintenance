<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="maintenance_equipment_document_document_form" model="ir.ui.view">
        <field name="name">maintenance.equipment.document.document.form</field>
        <field name="model">maintenance.equipment.document.document</field>
        <field name="arch" type="xml">
            <form string="Document" create="false" edit="false" delete="false">
                <sheet>
                    <group>
                        <group name="document_group">
                            <field name="description" />
                            <field name="document_id" widget="many2many_binary" />
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
